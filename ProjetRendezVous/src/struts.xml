<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	<constant name="struts.devMode" value="false" />

	<package name="ProjetRendezVous" namespace="/" extends="struts-default">
		<action name="Menu">
			<result> /menu/Menu.jsp</result>
			
		</action>
		<action name="valider" class="fr.entite.etudiant.ControlleurEtudiantAction" method="execute">
			<result name="success">/formulaire/ControlleurEtudiant.jsp</result>
			
		</action>
		<action name="compte">
			<result>/formulaire/formulaireEtudiant.jsp</result>
			
		</action>
	      <action name="Connexion">
				<result>/connexion/Connexion.jsp</result>
			
		</action>
		<action name="identificationUtilisateur" class="fr.beanAction.UtilisateurAction" method="identifierUtilisateurs">
            <result name="success">/connexion/success.jsp</result>
        	<result name="error">/connexion/Connexion.jsp</result>
         </action>
		
	</package>

</struts>